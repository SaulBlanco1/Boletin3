<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView_idpreg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginTop="16dp"
        android:text="idpreg"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_pregu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="pregu"
        app:layout_constraintEnd_toStartOf="@+id/textView_respcorr"
        app:layout_constraintStart_toEndOf="@+id/textView_idpreg"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="14dp" />

    <TextView
        android:id="@+id/textView_respcorr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="72dp"
        android:text="respcorr"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="14dp" />

</androidx.constraintlayout.widget.ConstraintLayout>